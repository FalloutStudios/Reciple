modules that are in package.json dependencies and dev dependencies\n        this.packageJsonPath = path.join(process.cwd(), 'package.json');\n\n        return super.onStart(client);\n    }\n}\n\nexport default new NPMLoader();\n```\n","changelog":null,"classes":[{"id":24,"name":"RecipleNPMLoader","source":{"line":57,"file":"module.ts","path":".","url":"https://github.com/FalloutStudios/Reciple/blob/5235eea/packages/npm-loader/src/module.ts#L57"},"namespaceParentId":null,"comment":{"description":null,"blockTags":[],"modifierTags":[]},"external":false,"abstract":false,"extendsType":null,"implementsType":[{"kind":"reference","id":null,"name":"unknown","packageName":null,"typeArguments":[]},{"kind":"reference","id":18,"name":"RecipleNPMLoaderOptions","packageName":"@reciple/npm-loader","typeArguments":[]}],"typeParameters":[],"construct":{"id":50,"name":"constructor","source":{"line":74,"file":"module.ts","path":".","url":"https://github.com/FalloutStudios/Reciple/blob/5235eea/packages/npm-loader/src/module.ts#L74"},"comment":{"description":null,"blockTags":[],"modifierTags":[]},"parentId":24,"accessibility":"public","parameters":[{"id":52,"name":"options","comment":{"description":null,"blockTags":[],"modifierTags":[]},"rest":false,"optional":true,"type":{"kind":"reference","id":18,"name":"RecipleNPMLoaderOptions","packageName":"@reciple/npm-loader","typeArguments":[]}}]},"properties":[{"id":57,"name":"client","source":{"line":63,"file":"module.ts","path":".","url":"https://github.com/FalloutStudios/Reciple/blob/5235eea/packages/npm-loader/src/module.ts#L63"},"comment":{"description":null,"blockTags":[],"modifierTags":[]},"parentId":24,"accessibility":"public","abstract":false,"static":false,"readonly":false,"optional":false,"type":{"kind":"reference","id":null,"name":"RecipleClient","packageName":null,"typeArguments":[]}},{"id":61,"name":"disableVersionChecks","source":{"line":68,"file":"module.ts","path":".","url":"https://github.com/FalloutStudios/Reciple/blob/5235ee