{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "docs"
        ],
        "accessToken": "MzJjYTY5YWMtZjgzNi00NGJkLWFkZGUtY2VjMzE5OWZjYTVjfHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/bin"
      ],
      "inputs": [
        "{projectRoot}/src/**/*"
      ]
    },
    "docs": {
      "dependsOn": [
        "^docs"
      ],
      "outputs": [
        "{projectRoot}/docs/docs.json"
      ],
      "inputs": [
        "{projectRoot}/src/**/*"
      ]
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "sharedGlobals": [],
    "production": [
      "default"
    ]
  }
}
