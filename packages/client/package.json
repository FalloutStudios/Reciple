{
  "name": "@reciple/client",
  "description": "Reciple client and builder classes",
  "homepage": "https://reciple.js.org/docs/client",
  "license": "GPL-3.0",
  "version": "7.6.0-dev.2",
  "main": "./dist/index.js",
  "module": "./dist/esm.mjs",
  "types": "./dist/index.d.ts",
  "keywords": [
    "reciple",
    "discord.js",
    "handler",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FalloutStudios",
    "directory": "packages/client"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "npx rimraf ./dist",
    "build": "npm run clean && npx tsc",
    "docs": "npx docgen -i ./src/index.ts -c ./docs/custom.json -o ./docs/docs.json --typescript"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@sapphire/shapeshift": "^3.9.0",
    "fallout-utility": "^2.5.0",
    "kleur": "^4.1.5",
    "semver": "^7.5.1",
    "strip-ansi": "^6.0.1",
    "ts-mixer": "^6.0.2"
  },
  "devDependencies": {
    "@types/semver": "^7.5.0",
    "discord.js": "^14.11.0"
  },
  "peerDependencies": {
    "discord.js": "^14.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
