{
  "name": "create-reciple",
  "description": "A Reciple Discord bot project builder",
  "license": "GPL-3.0",
  "version": "9.0.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "bin": "./dist/bin.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/Reciple",
    "directory": "packages/create-reciple"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "docs": "node ../docgen/dist/bin.js -i ./src/index.ts -o ./docs/docs.json"
  },
  "files": [
    "dist",
    "templates",
    "assets",
    "README.md"
  ],
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@reciple/utils": "^9.0.0",
    "commander": "^12.1.0",
    "fallout-utility": "^2.9.1"
  },
  "devDependencies": {
    "@reciple/core": "^9.0.0",
    "@types/node": "^20.12.12",
    "discord.js": "^14.15.2",
    "nodemon": "^3.1.2",
    "reciple": "^9.0.0",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  }
}
