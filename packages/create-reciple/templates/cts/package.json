{
  "name": "reciple-app",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "build": "rimraf ./modules && tsc",
    "start": "reciple -c reciple.cjs",
    "build:start": "SCRIPT_RUN build && reciple -c reciple.cjs",
    "dev": "reciple --watch --watch-preload-script build -c reciple.cjs"
  },
  "dependencies": {
    "@reciple/core": "RECIPLE_CORE",
    "discord.js": "DISCORDJS",
    "reciple": "RECIPLE"
  },
  "devDependencies": {
    "@types/node": "TYPES_NODE",
    "typescript": "TYPESCRIPT",
    "rimraf": "RIMRAF"
  }
}