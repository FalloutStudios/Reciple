{
  "name": "reciple-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "BIN_EXEC rimraf ./modules && BIN_EXEC tsc",
    "start": "BIN_EXEC reciple",
    "dev": "BIN_EXEC tsc-watch -p ./tsconfig.json --onCompilationStarted \"BIN_EXEC rimraf ./modules\" --onSuccess \"SCRIPT_RUN start\""
  },
  "dependencies": {
    "discord.js": "DISCORDJS",
    "reciple": "RECIPLE"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "tsc-watch": "TSC_WATCH",
    "typescript": "TYPESCRIPT",
    "rimraf": "RIMRAF"
  }
}