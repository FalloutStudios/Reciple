{
  "name": "@reciple/docgen",
  "description": "Parses typescript files and generate json output",
  "license": "GPL-3.0",
  "version": "8.2.1",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "reciple-docgen": "./dist/bin.js"
  },
  "keywords": [
    "reciple",
    "discord.js",
    "handler",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/Reciple",
    "directory": "packages/npm-loader"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "docs": "node ./dist/bin.js -i ./src/index.ts -o ./docs/docs.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@discordjs/collection": "^2.1.0",
    "@reciple/utils": "^8.2.1",
    "commander": "^12.0.0",
    "typedoc": "^0.25.13",
    "typedoc-json-parser": "^10.0.0"
  }
}
