{
  "name": "reciple",
  "description": "Reciple is a Discord.js bot framework",
  "homepage": "https://reciple.js.org/docs/reciple",
  "license": "GPL-3.0",
  "version": "8.1.6",
  "main": "./dist/index.js",
  "module": "./dist/esm.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/bin.mjs",
  "keywords": [
    "reciple",
    "discord.js",
    "handler",
    "commands"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/Reciple",
    "directory": "packages/reciple"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm.mjs",
      "require": "./dist/index.js"
    },
    "./config.mjs": {
      "types": "./static/config.d.mts",
      "import": "./static/config.mjs"
    }
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "docs": "node ../docgen/dist/bin.js -c ./docs/custom.json -i ./src/exports.ts -o ./docs/docs.json"
  },
  "files": [
    "dist",
    "static",
    "README.md"
  ],
  "dependencies": {
    "@reciple/update-checker": "^8.0.2",
    "@reciple/utils": "^8.0.3",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "fallout-utility": "^2.7.1",
    "globby": "^13.2.1",
    "lodash.mergewith": "^4.6.2",
    "micromatch": "^4.0.5",
    "prompts": "^2.4.2",
    "semver": "^7.5.4",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@reciple/core": "^8.1.6",
    "@types/micromatch": "^4.0.4",
    "@types/semver": "^7.5.4",
    "discord.js": "^14.13.0"
  },
  "peerDependencies": {
    "@reciple/core": "^8.0.0",
    "discord.js": "^14.7.1"
  },
  "gitHead": "c5620aa5f046aa426b832c052a4f933817270573"
}
