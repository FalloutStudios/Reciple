{
  "name": "reciple",
  "description": "Reciple is a Discord.js bot framework",
  "homepage": "https://reciple.js.org/docs/reciple",
  "license": "GPL-3.0",
  "version": "8.3.2",
  "main": "./dist/index.js",
  "module": "./dist/esm.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/bin.mjs",
  "keywords": [
    "reciple",
    "discord.js",
    "handler",
    "commands"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/Reciple",
    "directory": "packages/reciple"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm.mjs",
      "require": "./dist/index.js"
    },
    "./config.mjs": {
      "types": "./static/config.d.mts",
      "import": "./static/config.mjs"
    }
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "docs": "node ../docgen/dist/bin.js -c ./docs/custom.json -i ./src/exports.ts -o ./docs/docs.json"
  },
  "files": [
    "dist",
    "static",
    "README.md"
  ],
  "dependencies": {
    "-": "^0.0.1",
    "@reciple/update-checker": "^9.0.0",
    "@reciple/utils": "^8.2.1",
    "chokidar": "^3.6.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "fallout-utility": "^2.8.0",
    "globby": "^14.0.1",
    "micromatch": "^4.0.5",
    "prompts": "^2.4.2",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "@reciple/core": "^8.3.2",
    "@types/micromatch": "^4.0.7",
    "@types/semver": "^7.5.8",
    "discord.js": "^14.14.1"
  },
  "peerDependencies": {
    "@reciple/core": "^8.0.0",
    "discord.js": "^14.7.1"
  },
  "gitHead": "c5620aa5f046aa426b832c052a4f933817270573"
}
