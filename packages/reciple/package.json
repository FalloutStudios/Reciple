{
  "name": "reciple",
  "description": "Reciple is a Discord.js bot framework",
  "homepage": "https://reciple.js.org/docs/reciple",
  "license": "GPL-3.0",
  "version": "7.8.2",
  "main": "./bin/index.js",
  "module": "./bin/esm.mjs",
  "types": "./bin/index.d.ts",
  "bin": "./bin/bin.mjs",
  "keywords": [
    "reciple",
    "discord.js",
    "handler",
    "commands"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FalloutStudios",
    "directory": "packages/reciple"
  },
  "exports": {
    ".": {
      "types": "./bin/index.d.ts",
      "import": "./bin/esm.mjs",
      "require": "./bin/index.js"
    }
  },
  "scripts": {
    "clean": "npx rimraf ./bin",
    "build": "npm run clean && npx tsc",
    "docs": "npx docgen -i ./src/exports.ts -c ./docs/custom.json -o ./docs/docs.json --typescript"
  },
  "files": [
    "bin",
    "static",
    "README.md"
  ],
  "dependencies": {
    "@reciple/client": "^7.7.2",
    "@reciple/update-checker": "^7.1.0",
    "commander": "^11.0.0",
    "dotenv": "^16.2.0",
    "fallout-utility": "^2.5.1",
    "globby": "^13.1.4",
    "kleur": "^4.1.5",
    "lodash.mergewith": "^4.6.2",
    "micromatch": "^4.0.5",
    "prompts": "^2.4.2",
    "semver": "^7.5.2",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@types/lodash.mergewith": "^4.6.7",
    "@types/micromatch": "^4.0.2",
    "@types/prompts": "^2.4.4",
    "@types/semver": "^7.5.0",
    "discord.js": "^14.11.0"
  },
  "peerDependencies": {
    "discord.js": "^14.7.1"
  },
