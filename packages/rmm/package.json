{
  "name": "@reciple/rmm",
  "private": true,
  "description": "Module manager for the default Reciple CLI",
  "homepage": "https://reciple.js.org/docs/rmm",
  "type": "module",
  "license": "GPL-3.0",
  "version": "7.0.4-dev.3",
  "module": "./bin/index.js",
  "types": "./bin/index.d.ts",
  "bin": {
    "rmm": "./bin/bin.js"
  },
  "keywords": [
    "reciple",
    "discord.js",
    "handler"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FalloutStudios",
    "directory": "packages/rmm"
  },
  "exports": {
    ".": {
      "types": "./bin/index.d.ts",
      "import": "./bin/index.js"
    }
  },
  "scripts": {
    "clean": "npx rimraf ./bin",
    "build": "npm run clean && npx tsc",
    "docs": "npx docgen -i ./src/index.ts -c ./docs/custom.json -o ./docs/docs.json --typescript"
  },
  "files": [
    "bin",
    "README.md"
  ],
  "dependencies": {
    "@types/adm-zip": "^0.5.0",
    "adm-zip": "^0.5.10",
    "axios": "^1.3.3",
    "chalk": "^5.2.0",
    "cli-spinners": "^2.7.0",
    "commander": "^10.0.0",
    "fallout-utility": "^2.1.3",
    "ora": "^6.1.2",
    "prompts": "^2.4.2",
    "reciple": "^7.0.13-dev.3",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.2",
    "@types/semver": "^7.3.13"
  }
}
