{
  "name": "@reciple/update-checker",
  "description": "Check updates from npm registry",
  "license": "GPL-3.0",
  "version": "9.1.0",
  "main": "./dist/index.js",
  "module": "./dist/esm.mjs",
  "types": "./dist/index.d.ts",
  "keywords": [
    "reciple",
    "npm",
    "update"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/Reciple",
    "directory": "packages/update-checker"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "docs": "node ../docgen/dist/bin.js -i ./src/index.ts -o ./docs/docs.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "package-json": "^10.0.0",
    "semver": "^7.6.1"
  },
  "devDependencies": {
    "@types/semver": "^7.5.8"
  }
}
