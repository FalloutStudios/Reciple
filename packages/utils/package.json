{
  "name": "@reciple/utils",
  "description": "Utilities used by reciple modules",
  "license": "GPL-3.0",
  "version": "7.0.0",
  "main": "./dist/index.js",
  "module": "./dist/esm.mjs",
  "types": "./dist/index.d.ts",
  "keywords": [
    "reciple",
    "utility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FalloutStudios",
    "directory": "packages/utils"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm.mjs",
      "require": "./dist/index.js"
    },
    "./tsconfig.json": "./assets/tsconfig.json"
  },
  "scripts": {
    "clean": "npx rimraf ./dist",
    "build": "npm run clean && npx tsc",
    "docs": "npx docgen -i ./src/index.ts -c ./docs/custom.json -o ./docs/docs.json --typescript"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "fallout-utility": "^2.7.0"
  }
}
