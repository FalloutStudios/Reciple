{
  "name": "@reciple/utils",
  "description": "Utilities used by reciple modules",
  "license": "GPL-3.0",
  "version": "8.0.0-dev.4",
  "main": "./dist/index.js",
  "module": "./dist/esm.mjs",
  "types": "./dist/index.d.ts",
  "keywords": [
    "reciple",
    "utility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FalloutStudios",
    "directory": "packages/utils"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm.mjs",
      "require": "./dist/index.js"
    },
    "./tsconfig.json": "./assets/tsconfig.json"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "docs": "node ../docgen/dist/bin.js -i ./src/index.ts -o ./docs/docs.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@discordjs/collection": "^1.5.2",
    "discord-api-types": "^0.37.60",
    "dotenv": "^16.3.1",
    "fallout-utility": "^2.7.1"
  },
  "devDependencies": {
    "discord.js": "^14.13.0"
  }
}
