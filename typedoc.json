{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/index.ts"
  ],
  "entryPointStrategy": "Expand",
  "out": "docs",
  "plugin": [
    "typedoc-plugin-mdn-links",
    "typedoc-plugin-markdown",
    "typedoc-just-the-docs-theme"
  ],
  "theme": "just-the-docs",
  "tsconfig": "tsconfig.json",
  "name": "Reciple",
  "githubPages": false
}